<?xml version="1.0" encoding="UTF-8"?>

<protounitsounddef>
	<protounit name="Unktehi Winyela">
		<soundtype name="Select">
			<soundset name="CowSelect"></soundset>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="CowGrunt"></soundset>
		</soundtype>
		<soundtype name="Hit">
			<damagetypelogic>
				<choice name="Hack">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Slash">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Pierce">
					<soundset name="PierceFlesh"></soundset>
				</choice>
				<choice name="Crush">
					<soundset name="CrushFlesh"></soundset>
				</choice>
			</damagetypelogic>
		</soundtype>
		<soundtype name="Death">
			<soundset name="CowDeath"></soundset>
		</soundtype>
		<soundtype name="Acknowledge">
			<soundset name="CowAcknowledge"></soundset>
		</soundtype>
