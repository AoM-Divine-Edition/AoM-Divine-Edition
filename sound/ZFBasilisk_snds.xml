<?xml version="1.0" encoding="UTF-8"?>

<protounitsounddef>
	<protounit name="ZFBasilisk">
		<soundtype name="Select">
			<soundset name="BasiliskSelect"></soundset>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="BasiliskGrunt"></soundset>
		</soundtype>
		<soundtype name="Hit">
			<damagetypelogic>
				<choice name="Crush">
					<soundset name="CrushFlesh"></soundset>
				</choice>
				<choice name="Fire">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Hack">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Pierce">
					<soundset name="PierceFlesh"></soundset>
				</choice>
				<choice name="Slash">
					<soundset name="HackFlesh"></soundset>
				</choice>
			</damagetypelogic>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="MythUnitBirth"></soundset>
		</soundtype>
		<soundtype name="Death">
			<soundset name="BasiliskDeath"></soundset>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="BasiliskMove"></soundset>
				</choice>
				<choice name="enemy">
					<soundset name="BasiliskAttack"></soundset>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>
