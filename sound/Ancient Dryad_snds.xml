<?xml version="1.0"?>

<protounitsounddef>
	<protounit name ="Ancient Dryad">
		<soundtype name ="Select">
			<soundset name ="AncientDryadSelect"></soundset>
		</soundtype>
		<soundtype name ="Grunt">
			<soundset name ="AncientDryadHit"></soundset>
		</soundtype>
		<soundtype name ="Hit">
			<damagetypelogic>
				<choice name ="Crush">
					<soundset name ="CrushWood"></soundset>
				</choice>
				<choice name ="Fire">
					<soundset name ="TreeHit"></soundset>
				</choice>
				<choice name ="Hack">
					<soundset name ="TreeHit"></soundset>
				</choice>
				<choice name ="Pierce">
					<soundset name ="PierceWood"></soundset>
				</choice>
				<choice name ="Slash">
					<soundset name ="TreeHit"></soundset>
				</choice>
			</damagetypelogic>
		</soundtype>
		<soundtype name ="Death">
			<soundset name ="AncientDryadDeath"></soundset>
		</soundtype>
		<soundtype name ="Creation">
			<soundset name ="MythUnitBirth"></soundset>
		</soundtype>
		<soundtype name ="Acknowledge">
			<targetlogic>
				<choice name ="default">
					<soundset name ="AncientDryadAcknowledge"></soundset>
				</choice>
				<choice name ="enemy">
					<soundset name ="AncientDryadAttack"></soundset>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>