						<choice name="Gold">
							<soundset name="VillagerRomanFemaleMine"></soundset>
						</choice>
						<choice name="Fish">
							<soundset name="VillagerRomanFemaleFish"></soundset>
						</choice>
						<choice name="Huntable">
							<soundset name="VillagerRomanFemaleHunt"></soundset>
						</choice>
						<choice name="Building">
							<soundset name="VillagerRomanFemaleRepair"></soundset>
						</choice>
					</targetlogic>
				</choice>
