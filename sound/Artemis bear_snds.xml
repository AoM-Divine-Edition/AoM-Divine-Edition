<?xml version="1.0" encoding="UTF-8"?>

<protounitsounddef>
	<protounit name="Artemis Bear">
		<soundtype name="Select">
			<soundset name="SetSelect"></soundset>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="BearGrunt"></soundset>
		</soundtype>
		<soundtype name="Hit">
			<damagetypelogic>
				<choice name="Hack">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Slash">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Pierce">
					<soundset name="PierceFlesh"></soundset>
				</choice>
				<choice name="Crush">
					<soundset name="CrushFlesh"></soundset>
				</choice>
			</damagetypelogic>
		</soundtype>
		<soundtype name="Death">
			<soundset name="BearDie"></soundset>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="SetAcknowledge"></soundset>
				</choice>
				<choice name="enemy">
					<soundset name="SetAttack"></soundset>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>
