<?xml version="1.0" encoding="UTF-8"?>

<protounitsounddef>
	<protounit name="Unwraveler">
		<soundtype name="Select">
				<soundset name="EldMythSelect"></soundset>
		</soundtype>
		<soundtype name="Grunt">
				<soundset name="GiantGrunt"></soundset>
		</soundtype>
		<soundtype name="Death">
				<soundset name="EldMythPray"></soundset>
		</soundtype>
		<soundtype name="Hit">
			<damagetypelogic>
				<choice name="Hack">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Slash">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Pierce">
					<soundset name="PierceFlesh"></soundset>
				</choice>
				<choice name="Crush">
					<soundset name="CrushFlesh"></soundset>
				</choice>
			</damagetypelogic>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="MythUnitBirth"></soundset>
		</soundtype>
		<soundtype name="Acknowledge">
					<targetlogic>
						<choice name="default">
							<soundset name="EldMythMove"></soundset>
						</choice>
						<choice name="enemy">
							<soundset name="EldMythAttack"></soundset>
						</choice>
						<choice name="Berry Bush">
							<soundset name="EldMythBerry"></soundset>
						</choice>
						<choice name="Farm">
							<soundset name="EldMythFarm"></soundset>
						</choice>
						<choice name="Tree">
							<soundset name="EldMythWood"></soundset>
						</choice>
						<choice name="Gold">
							<soundset name="EldMythGold"></soundset>
						</choice>
						<choice name="Herdable">
							<soundset name="EldMythHerd"></soundset>
						</choice>
						<choice name="Huntable">
							<soundset name="EldMythHunt"></soundset>
						</choice>
						<choice name="Building">
							<soundset name="EldMythBuild"></soundset>
						</choice>
					</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>
