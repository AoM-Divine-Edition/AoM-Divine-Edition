<?xml version="1.0" encoding="UTF-8"?>

<protounitsounddef>
	<protounit name="Jaguar">
		<soundtype name="Select">
			<soundset name="JaguarSelect"></soundset>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="JaguarGrunt"></soundset>
		</soundtype>
		<soundtype name="Hit">
			<damagetypelogic>
				<choice name="Crush">
					<soundset name="CrushFlesh"></soundset>
				</choice>
				<choice name="Fire">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Hack">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Pierce">
					<soundset name="PierceFlesh"></soundset>
				</choice>
				<choice name="Slash">
					<soundset name="HackFlesh"></soundset>
				</choice>
			</damagetypelogic>
		</soundtype>
		<soundtype name="Death">
			<soundset name="JaguarDeath"></soundset>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="JaguarBirth"></soundset>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="JaguarAcknowledge"></soundset>
				</choice>
				<choice name="enemy">
					<soundset name="JaguarAcknowledge"></soundset>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>
