<?xml version="1.0" encoding="UTF-8"?>

<protounitsounddef>
	<protounit name="Decurion">
		<soundtype name="Select">
			<soundset name="MilitaryRomanSelect"></soundset>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="CavalryGrunt"></soundset>
		</soundtype>
		<soundtype name="Hit">
			<damagetypelogic>
				<choice name="Crush">
					<soundset name="CrushFlesh"></soundset>
				</choice>
				<choice name="Fire">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Hack">
					<soundset name="MetalSlice"></soundset>
				</choice>
				<choice name="Pierce">
					<soundset name="PierceFlesh"></soundset>
				</choice>
				<choice name="Slash">
					<soundset name="MetalSlice"></soundset>
				</choice>
			</damagetypelogic>
		</soundtype>
		<soundtype name="Death">
			<soundset name="MaleDeath"></soundset>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="MilitaryBirth"></soundset>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="MilitaryRomanAcknowledge"></soundset>
				</choice>
				<choice name="enemy">
					<soundset name="MilitaryRomanAttack"></soundset>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>
