<?xml version="1.0"?>

<protounitsounddef>
	<protounit name ="MudGiant">
		<soundtype name ="Select">
			<soundset name ="MudGiantSelect"></soundset>
		</soundtype>
		<soundtype name ="Grunt">
			<soundset name ="MudGiantGrunt"></soundset>
		</soundtype>
		<soundtype name ="Hit">
			<damagetypelogic>
				<choice name ="Crush">
					<soundset name ="CrushWood"></soundset>
				</choice>
				<choice name ="Fire">
					<soundset name ="TreeHit"></soundset>
				</choice>
				<choice name ="Hack">
					<soundset name ="TreeHit"></soundset>
				</choice>
				<choice name ="Pierce">
					<soundset name ="PierceWood"></soundset>
				</choice>
				<choice name ="Slash">
