<?xml version="1.0" encoding="UTF-8"?>

<protounitsounddef>
	<protounit name="Caucasian Eagle">
		<soundtype name="Select">
			<soundset name="SBirdSelect"></soundset>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="SBirdGrunt"></soundset>
		</soundtype>
		<soundtype name="Hit">
			<damagetypelogic>
				<choice name="Crush">
					<soundset name="CrushFlesh"></soundset>
				</choice>
				<choice name="Fire">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Hack">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Pierce">
					<soundset name="PierceFlesh"></soundset>
				</choice>
				<choice name="Slash">
					<soundset name="HackFlesh"></soundset>
				</choice>
			</damagetypelogic>
		</soundtype>
		<soundtype name="Death">
			<soundset name="SBirdDie"></soundset>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="MythUnitBirth"></soundset>
		</soundtype>
		<soundtype name="Acknowledge">
			<soundset name="SBirdAcknowledge"></soundset>
		</soundtype>
	</protounit>
</protounitsounddef>
