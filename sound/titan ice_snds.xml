<?xml version="1.0"?>

<protounitsounddef>
	<protounit name ="Titan Ice">
		<soundtype name ="Select">
			<soundset name ="MountainGiantSelect"></soundset>
		</soundtype>
		<soundtype name ="Attack">
			<soundset name ="MountainGiantAttack"></soundset>
		</soundtype>
		<soundtype name ="Grunt">
			<soundset name ="TitanNorseGrunt"></soundset>
		</soundtype>
		<soundtype name ="Hit">
			<damagetypelogic>
				<choice name ="Crush">
					<soundset name ="CrushFlesh"></soundset>
				</choice>
				<choice name ="Fire">
					<soundset name ="HackFlesh"></soundset>
				</choice>
				<choice name ="Hack">
					<soundset name ="HackFlesh"></soundset>
				</choice>
				<choice name ="Pierce">
					<soundset name ="PierceFlesh"></soundset>
				</choice>
				<choice name ="Slash">
					<soundset name ="HackFlesh"></soundset>
				</choice>
			</damagetypelogic>
		</soundtype>
		<soundtype name ="Death">
			<soundset name ="CyclopsDeath"></soundset>
		</soundtype>
		<soundtype name ="Creation">
			<soundset name ="HeroBirth"></soundset>
		</soundtype>
		<soundtype name ="Acknowledge">
			<targetlogic>
				<choice name ="default">
					<soundset name ="MountainGiantAcknowledge"></soundset>
				</choice>
				<choice name ="enemy">
					<soundset name ="MountainGiantAttack"></soundset>
				</choice>
			</targetlogic>
		</soundtype>
		<soundtype name ="Exists">
			<soundset name ="FrostGiantExist"></soundset>
		</soundtype>
	</protounit>
</protounitsounddef>